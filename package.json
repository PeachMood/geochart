{
  "name": "geochart",
  "version": "0.0.0",
  "description": "Library for plotting charts in the area of geonavigation",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run build:js && npm run build:types",
    "build:js": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "lint:fix": "eslint --fix src/**/*.{js,ts,jsx,tsx}",
    "prettier": "prettier --write src/**/* ./*.{json,*.json} !package-lock.json -u --no-error-on-unmatched-pattern",
    "format": "npm run prettier && npm run lint:fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeachMood/geochart.git"
  },
  "keywords": [
    "charts",
    "geology",
    "geonavigation"
  ],
  "author": "Ann Voronova",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeachMood/geochart/issues"
  },
  "homepage": "https://github.com/PeachMood/geochart#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
