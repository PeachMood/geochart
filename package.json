{
  "name": "geochart",
  "version": "0.0.1",
  "description": "Library for plotting charts in the area of geonavigation",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "module": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts"
    }
  },
  "scripts": {
    "start": "rollup -c -w",
    "build": "rollup -c",
    "lint": "eslint --fix src/**/*.{js,ts,jsx,tsx}",
    "prettier": "prettier --write src/**/* ./*.{json,*.json} !package-lock.json -u --no-error-on-unmatched-pattern",
    "format": "npm run prettier && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PeachMood/geochart.git"
  },
  "keywords": [
    "charts",
    "geology",
    "geonavigation"
  ],
  "author": "Ann Voronova",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PeachMood/geochart/issues"
  },
  "homepage": "https://github.com/PeachMood/geochart#readme",
  "devDependencies": {
    "@babel/core": "^7.24.3",
    "@babel/plugin-transform-runtime": "^7.24.3",
    "@babel/preset-env": "^7.24.3",
    "@babel/preset-react": "^7.24.1",
    "@babel/preset-typescript": "^7.24.1",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^7.3.1",
    "babel-core": "^6.26.3",
    "babel-runtime": "^6.26.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@visx/axis": "^3.5.0",
    "@visx/grid": "^3.5.0",
    "@visx/scale": "^3.5.0",
    "@visx/shape": "^3.5.0",
    "@visx/text": "^3.3.0",
    "classnames": "^2.3.2",
    "victory": "^36.6.12"
  }
}
